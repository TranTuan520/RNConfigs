{
  "name": "RNConfigs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-staging": " react-native run-android ENVFILE=.env.staging",
    "android-develop": " react-native run-android ENVFILE=.env.development",
    "android-product": " react-native run-android ENVFILE=.env.production",
    "android:clean": "cd android && ./gradlew clean",
    "android:build-release-apk": "cd android && ENVFILE=.env.production ./gradlew assembleRelease && open app/build/outputs/apk/release",
    "android:build-release-bundle": "cd android && ENVFILE=.env.production ./gradlew bundleRelease && open app/build/outputs/bundle/release",
    "xcode:open": "cd ios && open RNConfigs.xcworkspace",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/drawer": "^6.5.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.1",
    "@reduxjs/toolkit": "^1.8.5",
    "i18next": "^21.9.2",
    "react": "18.1.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.1",
    "react-native-config": "^1.4.6",
    "react-native-flipper": "^0.164.0",
    "react-native-gesture-handler": "^2.6.1",
    "react-native-localize": "^2.2.4",
    "react-native-reanimated": "^2.10.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.3.4",
    "react-native-screens": "^3.17.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.1",
    "rn-animated-switch": "^1.0.8",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.70.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.72.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
